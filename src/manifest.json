{
    "manifest_version": 3,
    "name": "One Tab Only",
    "version": "1.0",
    "description": "Limits the browser to a single tab at any time.",
    "permissions": ["tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/plasma_16px.png",
        "48": "icons/plasma_48px.png",
        "128": "icons/plasma_128px.png"
    }
}
